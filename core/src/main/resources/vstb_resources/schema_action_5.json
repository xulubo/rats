{
	"type":"object",
	"$schema": "http://json-schema.org/draft-03/schema",
	"required":false,
	"additionalProperties": false,
	"properties":{
		"apiRevision": {
			"type":"string",
			"required":false
		},
		"apiVersion": {
			"type":"string",
			"required":false
		},
		"payloadTTL": {
			"type":"number",
			"required":false
		},
		"payload": {
			"type":"array",
			"required":false,
			"items":
				{
					"type":"object",
					"required":false,
					"properties":{
						"categoryAttributes": {
							"type":"object",
							"required":false,
							"properties":{
								"isSubscribable": {
									"type":"boolean",
									"required":false
								}
							}
						},
						"contextPaths": {
							"type":"array",
							"required":false,
							"items":
								{
									"type":"string",
									"required":false
								}
							

						},
						"description": {
							"type":"string",
							"required":false
						},
						"extendedAttributes": {
							"type":"array",
							"required":false,
							"items":
								{
									"type":"object",
									"required":false,
									"properties":{
										"filterId": {
											"type":"string",
											"required":false
										}
									}
								}
							

						},
						"groupType": {
							"type":"string",
							"required":false
						},
						"iconCreatedDateTime": {
							"type":"string",
							"required":false
						},
						"iconURL": {
							"type":"string",
							"required":false
						},
						"id": {
							"type":"number",
							"required":false
						},
						"locale": {
							"type":"string",
							"required":false
						},
						"name": {
							"type":"string",
							"required":false
						},
						"price": {
							"type":"string",
							"required":false
						}
					}
				}
			

		},
		"status": {
			"type":"number",
			"required":false
		},
		"systemTime": {
			"type":"integer",
			"required":false
		}
	}
}
