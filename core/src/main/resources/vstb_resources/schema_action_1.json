			"type": "string"
		},

		"allowedRoamingCountries": 
		{
			"type": "array",
			"items": 
			{
				"type": "string"
			}
		},

		"appReportingAuthToken": 
		{
			"type": "string"
		},

		"applicationExpiryDate": 
		{
			"type": "string"
		},

		"mak": 
		{
			"type": "string"
		},

		"optInStatusMap": 
